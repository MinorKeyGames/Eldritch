	@@^B = "Shoggoth"
	@@^C = "Neutral"
	@@&A = "Shopkeeper"
	@@^B = "Dagon"
	@@^C = "Neutral"
	@@&A = "Shopkeeper"
	@@^B = "Bug"
	@@^C = "Hostile"
# Bug cons
	@@&A = "Bug"
	@@^B = "Player"
	@@^C = "Hostile"
