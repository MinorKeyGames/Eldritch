	@@^B = "Shopkeeper"
	@@^C = "Neutral"
	@@&A = "Dagon"
	@@^B = "Bug"
	@@^C = "Hostile"
# Shoggoth cons
	@@&A = "Shoggoth"
	@@^B = "Player"
	@@^C = "Hostile"
	@@&A = "Shoggoth"
	@@^B = "Dagon"
	@@^C = "Hostile"
	@@&A = "Shoggoth"
	@@^B = "Shopkeeper"
	@@^C = "Hostile"
	@@&A = "Shoggoth"
	@@^B = "Bug"
	@@^C = "Hostile"
# Shopkeeper cons
	@@&A = "Shopkeeper"
	@@^B = "Player"
	@@^C = "Neutral"
