		Value		= 1.0

		[HolySymbolPerk_TrueEnding]
		Event		= "HolySymbolPerk"
		Stat		= "TrueEnding"
		Function	= "Add"
		Value		= 1.0

		[SandalsPerk_TrueEnding]
		Event		= "SandalsPerk"
		Stat		= "TrueEnding"
		Function	= "Add"
		Value		= 1.0

		[ConsecrationPerk_TrueEnding]
		Event		= "ConsecrationPerk"
		Stat		= "TrueEnding"
		Function	= "Add"
		Value		= 1.0

