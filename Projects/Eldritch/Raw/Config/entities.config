	@@&A = "Player"
	@@^B = "Dagon"
	@@^C = "Hostile"
	@@&A = "Player"
	@@^B = "Shoggoth"
	@@^C = "Hostile"
	@@&A = "Player"
	@@^B = "Shopkeeper"
	@@^C = "Neutral"
	@@&A = "Player"
	@@^B = "Bug"
	@@^C = "Hostile"
# Dagon cons
	@@&A = "Dagon"
	@@^B = "Player"
	@@^C = "Hostile"
