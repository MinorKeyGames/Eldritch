	@ Reaction
	@@& = "SwampBookOnSpawnedReaction"
	@@& = "SwampBookOnFrobbedLockedReaction_WithKey"
	@@& = "SwampBookOnFrobbedLockedReaction_WithoutKey"
	@@& = "SwampBookOnFrobbedReaction"
	@@& = "SwampBookOnUnlockedReaction"

		[SwampBookOnSpawnedReaction]
