			EventName		= "PlaySound"
			Recipient		= "SelfPE"
			NumParameters	= &
			@ Parameter
			@@&Name			= "Sound"
			@@^Value		= "CampfireAmbientSFXPE"
			@@&Name			= "Attached"
			@@^Value		= "TruePE"

				[CampfireAmbientSFXPE]
