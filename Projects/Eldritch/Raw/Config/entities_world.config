				@@&			= "StopCampfireAmbientVFX"
				@@&			= "PlayCampfireStokedVFX"

					[SetStateStoked]
					ActionType		= "SendEvent"
					EventName		= "SetState"
					Recipient		= "SelfPE"
					NumParameters	= &
					@ Parameter
					@@&Name			= "NewState"
					@@^Value		= "StokedPE"

					[SetCampfireStokedFrobName]
