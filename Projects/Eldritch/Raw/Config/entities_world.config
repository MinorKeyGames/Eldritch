			Recipient		= "SelfPE"
			NumParameters	= &
			@ Parameter
			@@&Name			= "ParticleSystem"
			@@^Value		= "CampfireAmbientVFXPE"
			@@&Name			= "Attached"
			@@^Value		= "TruePE"

				[CampfireAmbientVFXPE]
				PEType		= "ConstantString"
				Value		= "PS_Campfire"

		[CampfireOnFrobbedAndStokedReaction]
