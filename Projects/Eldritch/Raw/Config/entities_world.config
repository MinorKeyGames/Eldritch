		NumActions	= &
		@ Action
		@@&			= "SetStateUnlit"
		@@&			= "BecomeNonFrobbable"
		@@&			= "RemoveLightAction"
		@@&			= "CampfireGiveStokedHealth"
		@@&			= "StopCampfireAmbientSFX"
		@@&			= "StopCampfireAmbientVFX"
		@@&			= "StopCampfireStokedVFX"

			[OnFrobbedAndStokedRule]
