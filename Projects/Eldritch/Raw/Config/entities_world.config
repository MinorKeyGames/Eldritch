	[FountainTransform]
	CanMove	= false

	[FountainCollision]
	IsStatic		= true
	HalfExtentsXY	= 0.25
	HalfExtentsZ	= 0.5546875	# 0.5 + .0625 - 0.0078125
	BlocksEntities	= true
	BlocksTrace		= true

	[FountainMesh]
	Mesh	= "Meshes/fountain.cms"
	OffsetZ	= -0.5625

