			ActionType		= "Selector"
			NumSelections	= &
			@ Selection
			@@&Condition	= "ShouldSpawnYogPE"
			@@^Action		= "SpawnYogAction"

				[ShouldSpawnYogPE]
				PEType	= "AND"
				InputA	= "PlayerHasAirplaneKeysPE"
				InputB	= "YogIsNotSpawnedPE"

					[YogIsNotSpawnedPE]
					PEType	= "NOT"
					Input	= "YogPE"

				[SpawnYogAction]
				ActionType			= "EldSpawnEntity"
				Entity				= "Yog"
				LocationOverridePE	= "PlayerLocationPE"

