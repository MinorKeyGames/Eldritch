	AirAcceleration			= 6.0
	TurnSpeed				= 180.0		# Degrees per second
	FollowValidateTime		= 1.0
	StepReachedThreshold	= 0.1
	TurnReachedThreshold	= 5.0		# Degrees
	IsFlying				= true
	MaxPathSteps			= 200

